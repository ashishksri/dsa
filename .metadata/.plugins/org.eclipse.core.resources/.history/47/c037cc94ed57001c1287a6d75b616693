package practise.queue;

public class Queue {
	int SIZE;
	int[] q;
	int front, rear;
	
	public Queue(int size) {
		this.SIZE=size;
		front=rear=-1;
		q = new int[this.SIZE];
	}
	
	public boolean isFull() {
		if(this.rear == this.SIZE-1) {
			return true;
		}
		return false;
	}
	
	public boolean isEmpty() {
		if(this.front == -1) {
			return true;
		}
		return false;
	}
	
	public void enqueue(int element) {
		
	}

}
