package tree.binary;

public class BinaryTree {
	
	public static class Node{
		int data;
		Node left;
		Node right;
		
		public Node(int value) {
			this.data = value;
			this.left = null;
			this.right = null;
		}
	}
	
	Node root;
	
	public void add(int data) {
		if(root == null) {
			root = new Node(data);
		}
	}

}
