package huffman.greedy;

public class Stack {
	
	int[] a;
	int front;
	
	public Stack(int size) {
		this.a = new int[size];
		this.front=-1;
	}
	
	public static void main(String[] args) {
		
	}
	
	public boolean isEmpty() {
		if(this.front==-1) {
			return true;
		}
		return false;
	}
	
	public void insert(int e) {
		if(this.front==-1) {
			this.front++;
		}
	}
	
	

}
